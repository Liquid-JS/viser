{
  "name": "@lq-viser/viser-ng",
  "version": "2.7.1",
  "description": "viser-ng is angular of viser.",
  "repository": "viserjs/viser-ng",
  "license": "MIT",
  "authors": "DT-FE",
  "files": [
    "lib",
    "es",
    "umd"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run build:ts && npm run build:cjs && npm run build:umd",
    "build:ts": "rimraf es && ngc",
    "build:cjs": "rimraf lib && cross-env NODE_ENV=commonjs BABEL_ENV=cjs babel ./es -d lib",
    "build:umd": "rimraf umd && cross-env BABEL_ENV=umd webpack --config webpack.config.js",
    "clean": "git clean -fdX .",
    "lint": "tslint src/**/*",
    "analyse": "cross-env NODE_ENV=analyse webpack --progress --config webpack.config.js"
  },
  "peerDependencies": {
    "@angular/common": "^10.1.6",
    "@angular/core": "^10.1.6",
    "tslib": "^2.0.3"
  },
  "dependencies": {
    "@lq-viser/viser": "^2.5.1",
    "@types/core-js": "*",
    "@types/node": "*",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@angular/common": "^10.1.6",
    "@angular/compiler": "^10.1.6",
    "@angular/compiler-cli": "^10.1.6",
    "@angular/core": "^10.1.6",
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-external-helpers": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "babel-loader": "^8.1.0",
    "colors": "^1.4.0",
    "cross-env": "^7.0.2",
    "gzip-size": "^5.1.1",
    "ng-packagr": "^10.1.2",
    "ora": "^5.1.0",
    "pretty-bytes": "^5.4.1",
    "rimraf": "^3.0.2",
    "tslib": "^1.10.0",
    "tslint": "^6.1.3",
    "typescript": "~4.0.3",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "zone.js": "^0.11.2"
  },
  "keywords": [
    "g2",
    "chart",
    "angular",
    "datavis"
  ],
  "publishConfig": {
    "access": "public"
  }
}
